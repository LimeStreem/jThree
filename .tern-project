{
  "ecmaVersion": 6,
  "libs": [
    "browser"
  ],
  "plugins": {
    "requirejs": {},
    "node": {},
    "modules": {},
    "es_modules":{},
    "doc_comment": {
      "fullDocs": true
    }
  },
  "loadEagerly": [
    "lib/**/*.js"
  ],
  "dontLoad" : [
    "src/**/*.js",
    "bin/**/*.js",
    "wwwroot/**/*.js",
    "coverage/**/*.js",
    "lit/**/*.js"
  ]
}
